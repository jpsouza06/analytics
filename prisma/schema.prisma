generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Error {
  id        String   @id @default(uuid())
  unit      String
  rotina    String
  modulo    String
  conteudo  String
  createdAt DateTime @default(now())

  @@map("errors")
}

model PageView {
  id        String   @id @default(uuid())
  rotina    String
  modulo    String
  createdAt DateTime @default(now())

  @@map("pageViews")
}

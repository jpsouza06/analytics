generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Error {
  id        String   @id @default(uuid())
  unit      String
  rotina    String
  modulo    String
  conteudo  String
  createdAt DateTime @default(now())

  @@map("errors")
}

model PageView {
  id        String   @id @default(uuid())
  rotina    String
  modulo    String
  createdAt DateTime @default(now())

  @@map("pageViews")
}

model Event {
  id        String   @id @default(uuid())
  nome      String
  conteudo  String
  createdAt DateTime @default(now())

  @@map("event")
}

model SystemStarted {
  id        String   @id @default(uuid())
  estado    String   @db.VarChar(2)
  modulo    String
  filial    String
  createdAt DateTime @default(now())

  @@map("systemStarted")
}
